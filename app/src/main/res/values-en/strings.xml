    </string>
    <string name="questionnaire_7_answer_1">None</string>
    <string name="questionnaire_7_answer_2">Very mild</string>
    <string name="questionnaire_7_answer_3">Mild</string>
    <string name="questionnaire_7_answer_4">Moderate</string>
    <string name="questionnaire_7_answer_5">Severe</string>
    <string name="questionnaire_7_answer_6">Very serve</string>

    <!-- Questionnaire8 -->
    <string name="questionnaire_8_number">Q8</string>
